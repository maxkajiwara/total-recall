{
  "installCommand": "npm ci && npm install --no-save --platform=linuxmusl --arch=x64 lightningcss",
  "buildCommand": "prisma generate && next build",
  "framework": "nextjs",
  "env": {
    "NODE_ENV": "production"
  },
  "build": {
    "env": {
      "SKIP_ENV_VALIDATION": "false"
    }
  },
  "ignoreCommand": "git diff HEAD^ HEAD --quiet ."
}